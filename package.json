{
  "name": "shop",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:sw": "yarn run build && yarn run server:sw",
    "build": "ng build --configuration production",
    "build:qa": "ng build --configuration=qa",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-mock-api-data": "tsc mock-server/buildScripts/generateData.ts && node mock-server/buildScripts/generateData.js",
    "preserver:mock": "yarn run generate-mock-api-data",
    "server:mock": "json-server --watch mock-server/buildScripts/db.json --port 3000",
    "server:sw": "http-server -p 5100 -c-1 dist/Shop -P http://10.0.75.1:5100",
    "doc": "compodoc -p tsconfig.doc.json -s"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "tsconfig": "<rootDir>/src/tsconfig.spec.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@angular/service-worker": "^13.1.2",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "bulma": "^0.9.3",
    "core-js": "^3.20.3",
    "rxjs": "^7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@angular/language-service": "^13.1.2",
    "@compodoc/compodoc": "^1.1.18",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "chalk": "^4.1.2",
    "codelyzer": "^6.0.2",
    "faker": "^5.5.3",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^27.4.7",
    "jest-preset-angular": "^11.0.1",
    "json-schema-faker": "0.5.0-rcv.32",
    "json-server": "^0.16.3",
    "karma": "~6.3.11",
    "protractor": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.4"
  }
}
