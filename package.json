{
  "name": "shop",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "ng serve",
    "start:sw": "yarn run build && yarn run server:sw",
    "build": "ng build --configuration production",
    "build:qa": "ng build --configuration=qa",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-mock-api-data": "tsc mock-server/buildScripts/generateData.ts && node mock-server/buildScripts/generateData.js",
    "preserver:mock": "yarn run generate-mock-api-data",
    "server:mock": "json-server --watch mock-server/buildScripts/db.json --port 3000",
    "server:sw": "http-server -p 5100 -c-1 dist/Shop -P http://10.0.75.1:5100",
    "doc": "compodoc -p src/tsconfig.app.json -s"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "@angular/service-worker": "^12.0.0",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/entity": "^12.0.0",
    "@ngrx/router-store": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "bulma": "^0.9.2",
    "core-js": "^3.8.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.19.9",
    "chalk": "^4.1.0",
    "codelyzer": "^6.0.0",
    "faker": "^5.1.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "json-schema-faker": "0.5.0-rcv.32",
    "json-server": "^0.16.3",
    "karma": "~6.3.2",
    "protractor": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.2.4"
  }
}
