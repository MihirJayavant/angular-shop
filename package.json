{
  "name": "shop",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5000",
    "start:sw": "yarn run build && yarn run server:sw",
    "build": "ng build --prod",
    "build:qa": "ng build --configuration=qa",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-mock-api-data": "tsc mock-server/buildScripts/generateData.ts && node mock-server/buildScripts/generateData.js",
    "preserver:mock": "yarn run generate-mock-api-data",
    "server:mock": "json-server --watch mock-server/buildScripts/db.json --port 3000",
    "server:sw": "http-server -p 5100 -c-1 dist/Shop -P http://10.0.75.1:5100",
    "doc": "compodoc -p src/tsconfig.app.json -s"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.4",
    "@angular/common": "^8.2.4",
    "@angular/compiler": "^8.2.4",
    "@angular/core": "^8.2.4",
    "@angular/forms": "^8.2.4",
    "@angular/platform-browser": "^8.2.4",
    "@angular/platform-browser-dynamic": "^8.2.4",
    "@angular/pwa": "0.803.2",
    "@angular/router": "^8.2.4",
    "@angular/service-worker": "^8.2.4",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/entity": "^8.3.0",
    "@ngrx/router-store": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "@ngrx/store-devtools": "^8.3.0",
    "bulma": "^0.7.5",
    "core-js": "^3.2.1",
    "immutable": "^4.0.0-rc.12",
    "rxjs": "^6.5.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.27",
    "@angular/cli": "~8.3.27",
    "@angular/compiler-cli": "^8.2.4",
    "@angular/language-service": "^8.2.4",
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.7.4",
    "chalk": "^2.4.2",
    "codelyzer": "~5.1.0",
    "faker": "^4.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "^7.1.1",
    "json-schema-faker": "^0.5.0-rc17",
    "json-server": "^0.15.1",
    "karma": "~4.3.0",
    "protractor": "~5.4.2",
    "tslint": "~5.19.0",
    "typescript": "~3.4.5"
  }
}